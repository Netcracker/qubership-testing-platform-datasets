<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <changeSet author="dataset" id="CREATE TABLE">
        <preConditions onError="WARN" onFail="MARK_RAN">
            <not>
                <tableExists tableName="DATASETLIST"/>
            </not>
        </preConditions>
        <createTable tableName="DATASETLIST">
            <column name="ID" type="${uuid_type}" defaultValueComputed="${uuid_function}">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="VISIBILITY_AREA_ID" type="${uuid_type}">
                <constraints nullable="false"/>
            </column>
            <column name="NAME" type="VARCHAR(255)"
                    remarks="Long names are difficult to read.">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>