{
  "info": {
    "name": "API Collection",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "PUT /testplan/va/{{vaId}}",
      "request": {
        "method": "PUT",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "auth": {
          "type": "basic",
          "basic": [
            { "key": "username", "value": "user", "type": "string" },
            { "key": "password", "value": "user", "type": "string" }
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\"name\": \"{{name}}\"\n}"
        },
        "url": {
          "raw": "{{base_url}}/api/projects",
          "host": ["{{base_url}}"],
          "path": ["api", "projects"]
        }
      }
    }
  ]
}
